import random

numero1 = float(10.5)

def miFuncion():
    numero2 = float(random.randrange(1,10))   #numero aleatorio del 1 al 10
    mensaje = "La suma de {} y {} es {}"
    print(mensaje.format(numero1,numero2,numero1+2))

miFuncion()
